{
  "name": "frr-web",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Luke Zirngibl",
  "version": "2.2.17",
  "keywords": [],
  "scripts": {
    "build": "tsc",
    "build:watch": "yarn build --watch",
    "babel:build": "yarn clean && babel ./src --out-dir lib --extensions '.ts','.tsx','.js','.jsx'",
    "babel:watch": "yarn babel:build --watch",
    "prepare": "install-peers && husky install",
    "build-types": "install-peers -f && tsc",
    "build:local": "tsc && yarn clean",
    "clean": "rm -rf node_modules/@material-ui/core node_modules/@material-ui/styles node_modules/react node_modules/react-dom node_modules/react-redux node_modules/styled-components node_modules/react-i18next",
    "patch": "yarn run build-types && git add -A && git commit -m 'patch' && npm version patch",
    "prettier:fix": "yarn prettier --write .",
    "prettier:check": "yarn prettier --check ."
  },
  "files": [
    "lib/**/*"
  ],
  "peerDependencies": {
    "@material-ui/core": "^4.9.10",
    "@material-ui/styles": "^4.9.10",
    "frr-util": "^1.0.3",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-i18next": "^11.8.12",
    "react-redux": "^7.0.0",
    "semantic-ui-react": "^2.0.3",
    "styled-components": "^5.0.0"
  },
  "dependencies": {
    "@devexperts/remote-data-ts": "^2.0.5",
    "date-fns": "^2.19.0",
    "fp-ts": "1.19.5",
    "monocle-ts": "2.0.0",
    "pdfjs-dist": "2.1.266",
    "react-click-away-listener": "^2.0.0",
    "react-color": "^2.19.3",
    "react-datepicker": "^3.6.0",
    "react-day-picker": "^7.4.8",
    "react-html-parser": "^2.0.2",
    "react-pdf": "^4.2.0",
    "react-responsive": "^8.2.0",
    "react-select": "^3.1.0",
    "react-virtualized": "^9.22.3",
    "typelevel-ts": "^0.4.0",
    "use-debounce": "3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/react": "^17.0.3",
    "@types/react-datepicker": "^3.1.7",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.0.0",
    "husky": ">=6",
    "install-peers-cli": "^2.2.0",
    "lint-staged": ">=10",
    "prettier": "^2.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^3.7.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md}": "prettier --write"
  }
}
