    "date-fns": "^2.19.0",
    "fp-ts": "^2.12.3",
    "lodash.range": "^3.2.0",
    "monocle-ts": "2.0.0",
    "pdfjs-dist": "^2.14.305",
    "react-click-away-listener": "^2.0.0",
    "react-color": "^2.19.3",
