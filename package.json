{
  "name": "frr-web",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Luke Zirngibl",
  "version": "1.1.38",
  "keywords": [],
  "scripts": {
    "build": "tsc",
    "build:babel": "yarn clean && babel ./src --out-dir lib --extensions '.ts','.tsx','.js','.jsx'",
    "build:watch": "yarn build:babel --watch",
    "prepare": "install-peers",
    "build-types": "install-peers -f && tsc",
    "build-types:watch": "yarn check-types --watch",
    "clean": "rm -rf node_modules/@material-ui/core node_modules/@material-ui/styles node_modules/react node_modules/react-dom node_modules/react-redux node_modules/redux-saga node_modules/redux-thunk node_modules/styled-components",
    "patch": "yarn run build-types && git add -A && git commit -m 'patch' && npm version patch"
  },
  "files": [
    "lib/**/*"
  ],
  "peerDependencies": {
    "@material-ui/core": "^4.9.10",
    "@material-ui/styles": "^4.9.10",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.0",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "styled-components": "4.3.2"
  },
  "dependencies": {
    "@devexperts/remote-data-ts": "^2.0.3",
    "css": "^3.0.0",
    "date-fns": "^2.17.0",
    "fp-ts": "1.19.5",
    "monocle-ts": "2.0.0",
    "pdfjs-dist": "2.1.266",
    "react-click-away-listener": "^2.0.0",
    "react-currency-input-field": "^2.0.0",
    "react-datepicker": "^3.4.1",
    "react-day-picker": "^7.4.8",
    "react-pdf": "^4.2.0",
    "react-select": "^3.1.0",
    "react-virtualized": "^9.21.2",
    "semantic-ui-react": "^0.88.2",
    "typelevel-ts": "^0.4.0",
    "use-debounce": "^3.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-typescript": "^7.13.0",
    "@types/react-datepicker": "^3.1.3",
    "@types/react-dom": "16.9.3",
    "@types/react-redux": "7.1.4",
    "@types/redux-saga": "^0.10.5",
    "@types/styled-components": "^4.1.4",
    "install-peers-cli": "^2.2.0",
    "prettier": "1.19.1",
    "ts-node": "^9.1.1",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "^3.7.2"
  }
